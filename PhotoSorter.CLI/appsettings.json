{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Microsoft.Hosting.Lifetime": "Trace"
      }
    }
  },
  "PhotoSorter": {
    "Action": "Sort",
    "SourceDirectory": "F:\\DCIM\\",
    "DestinationDirectory": "D:\\Inbox\\",
    "NamePattern": "%YYYY%\\%Comment%%YYYY%-%MM%-%DD%%Comment%",
    "UseFileCreationDateIfNoExif": true,
    "UseCopyInsteadOfMove": true,
    "ComplimentaryFileExtensionsToDelete": [ ".aac", ".LRF", ".html" ]
  }
}
